package com.aurionpro.junit.Test;

import static org.junit.Assert.assertEquals;

import org.junit.Test;

import com.aurionpro.junit.model.Calculator;

public class CalculatorTest {
	Calculator cal = new Calculator();

	@Test
	public void testAdd() {
		int sum = cal.add(2, 2);
		assertEquals(4, sum);
	}
}
